{
    "name": "ryudith/mezzio-custom-log",
    "description": "Library/middleware custom log for Mezzio.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Ryudith\\MezzioCustomLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryudith\\MezzioCustomLog\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ryudith"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^3.0",
        "laminas/laminas-diactoros": "^2.7",
        "symfony/console": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11",
        "guzzlehttp/guzzle": "^7.4.5"
    }
}
